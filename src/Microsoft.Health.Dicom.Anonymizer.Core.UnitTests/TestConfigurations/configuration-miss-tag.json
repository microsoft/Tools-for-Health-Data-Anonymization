{
    "rules": [
        {
            "method": "dateshift"
        }
    ],
    "defaultSettings": {
        "perturb": {
            "span": "1",
            "roundTo": 2,
            "rangeType": "Proportional",
            "distribution": "Uniform"
        },
        "dateShift": {
            "dateShiftKey": "123",
            "dateShiftScope": "SeriesInstance",
            "dateShiftRange": "50"
        }, // Scope could be SeriesInstance, StudyInstance and SOPInstance    
        "cryptoHash": {
            "cryptoHashKey": "123",
            "cryptoHashFunction": "sha256"
        }, // function could be sha128, sha256, sha512
        "redact": {
            "enablePartialAgesForRedact": false,
            "enablePartialDatesForRedact": false
        },
        "encrypt": {
            "encryptKey": "123456781234567812345678",
            "encryptFunction": "AES"
        },
        "substitute": {
            "replaceWith": "ANONYNOUS"
        }
    },
    "customizedSettings": {
        "perturbCustomerSetting": {
            "span": "1",
            "roundTo": 2,
            "rangeType": "Proportional",
            "distribution": "Uniform"
        }
    }
}