{
    "rules": [
        {
            "tag": "RetrieveAETitle",
            "method": "substitute",
            "params": { "replaceWith": "newValue" }
        },
        {
            "tag": "PatientAge",
            "method": "substitute",
            "params": { "replaceWith": "050Y" }
        },
        {
            "tag": "LongCodeValue",
            "method": "cryptoHash"
        }
    ],
    "defaultSettings": {
        "perturb": {
            "span": "1",
            "roundTo": 2,
            "rangeType": "Proportional",
            "distribution": "Uniform"
        },
        "dateShift": {
            "dateShiftKey": "123",
            "dateShiftScope": "SeriesInstance",
            "dateShiftRange": "50"
        },   
        "cryptoHash": {
            "cryptoHashKey": "123",
            "cryptoHashFunction": "sha256"
        },
        "redact": {
            "enablePartialAgesForRedact": false,
            "enablePartialDatesForRedact": false
        },
        "encrypt": {
            "encryptKey": "123456781234567812345678",
            "encryptFunction": "AES"
        },
        "substitute": {
            "replaceWith": "ANONYNOUS"
        }
    },
    "customizedSettings": {
        "perturbCustomerSetting": {
            "span": "1",
            "roundTo": 2,
            "rangeType": "Proportional",
            "distribution": "Uniform"
        }
    }
}